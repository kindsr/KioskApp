<UserControl x:Class="iBeautyNail.Pages.M050_ProcessingPayment"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrl="clr-namespace:iBeautyNail.Extensions.Controls"       
             xmlns:local="clr-namespace:iBeautyNail.Pages"
             xmlns:gif="clr-namespace:XamlAnimatedGif;assembly=XamlAnimatedGif"
             Style="{StaticResource ibnMain}"
             mc:Ignorable="d" 
             d:DesignHeight="1920" d:DesignWidth="1080">
    <Grid x:Name="ctPagebody">
        <Grid.RowDefinitions>
            <RowDefinition Height="1360" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Image Stretch="Fill" Grid.Row="0" Grid.Column="0" Grid.RowSpan="2"
               gif:AnimationBehavior.RepeatBehavior="Forever" gif:AnimationBehavior.SourceUri="pack://siteoforigin:,,,/Resources/Images/view3_1.png"></Image>
    </Grid>

</UserControl>
